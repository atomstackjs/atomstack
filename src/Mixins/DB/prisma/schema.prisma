datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "./client"
}

model TestModel {
  id                         String @id @default(cuid())
  plainText                  String
  encryptedText              String
  deterministicEncryptedText String
}
