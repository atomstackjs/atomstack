datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "./_client_"
}

model TestModel {
  id                         String  @id @default(cuid())
  plainText                  String?
  encryptedText              String?
  deterministicEncryptedText String?
}
