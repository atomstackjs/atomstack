{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "esModuleInterop": true,
    "moduleResolution": "NodeNext",
    "strict": true,
    "target": "es2022",
    "module": "NodeNext",
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
